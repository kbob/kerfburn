#!/usr/bin/python

# Slow pulsed dwell.  dt not a multiple of pi.

import sched_test

input = """dt=8500
re=n
rf=n
rv=y
R
Qd
W
"""

expected = """Kerfburn Back End, built [...]
Ready
V aa=0 dt=8500 ia=n il=0 lm=o lp=0 ls=m m0=0 ma=+0 oc=n oo=n pd=0 pi=0 pl=0 re=n rf=n ri=0 rl=n rm=n rp=n rq=n rr=n rs=n rv=y rw=n xd=+0 yd=+0 zd=+0
enqueue_atom_X: a = A_DISABLE_STEP
enqueue_atom_Y: a = A_DISABLE_STEP
enqueue_atom_Z: a = A_DISABLE_STEP
enqueue_atom_P: a = A_SET_MAIN_LASER_OFF
enqueue_atom_P: a = A_SET_VISIBLE_LASER_OFF
enqueue_atom_X: a = 8500
enqueue_atom_Y: a = 8500
enqueue_atom_Z: a = 8500
enqueue_atom_P: a = 8500
maybe_start_engine
start_engine
await_engine_stopped
"""

sched_test.test(input, expected)
